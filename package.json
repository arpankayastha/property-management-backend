{
    "name": "property-management-backend",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "build": "npm run tsc",
        "tsc": "tsc --project tsconfig.compile.json",
        "tsc:w": "tsc --project tsconfig.json -w",
        "start": "tsnd --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
        "start:prod": "cross-env NODE_ENV=production node dist/index.js",
        "typeorm": "tsed typeorm",
        "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
        "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
        "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
        "migration:create": "typeorm migration:create -n",
        "entity:create": "typeorm entity:create -n"
    },
    "dependencies": {
        "@tsed/ajv": "^6.73.4",
        "@tsed/common": "^6.73.4",
        "@tsed/core": "^6.73.4",
        "@tsed/di": "^6.73.4",
        "@tsed/exceptions": "^6.73.4",
        "@tsed/json-mapper": "^6.73.4",
        "@tsed/platform-express": "^6.73.4",
        "@tsed/schema": "^6.73.4",
        "@tsed/typeorm": "^6.73.4",
        "ajv": "^8.6.3",
        "aws-sdk": "^2.1010.0",
        "axios": "^0.27.2",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "canvas": "^2.8.0",
        "class-validator": "^0.13.1",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "firebase-admin": "^9.12.0",
        "ical": "^0.8.0",
        "jsonwebtoken": "^8.5.1",
        "merge-images": "^2.0.0",
        "method-override": "^3.0.0",
        "mime": "^2.5.2",
        "moment": "^2.29.1",
        "mysql": "^2.18.1",
        "node-ical": "^0.15.1",
        "node-schedule": "^2.1.0",
        "nodemailer": "^6.7.2",
        "qr-code-styling-node": "^1.5.0",
        "qrcode": "^1.4.4",
        "qs": "^6.11.0",
        "typeorm": "^0.2.38",
        "uuid": "^8.3.2",
        "vechain_sdk": "^1.0.4",
        "webp-converter": "^2.3.3",
        "window": "^4.2.7"
    },
    "devDependencies": {
        "@tsed/cli-plugin-typeorm": "3.11.6",
        "@types/compression": "^1.7.2",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/method-override": "^0.0.32",
        "@types/multer": "^1.4.7",
        "@types/node": "^16.10.2",
        "ts-node": "^10.2.1",
        "ts-node-dev": "^1.1.8",
        "tsconfig-paths": "^3.11.0",
        "typescript": "^4.4.3"
    },
    "tsed": {
        "packageManager": "npm",
        "convention": "default"
    }
}
